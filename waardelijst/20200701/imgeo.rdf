    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerkPlus"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerkPlus_bunker">
    <type xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
