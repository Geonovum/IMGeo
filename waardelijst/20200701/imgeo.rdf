    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Waterbak, zoals een zwembad of een dok.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerk"/>
