    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerkPlus"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerkPlus_voedersilo">
