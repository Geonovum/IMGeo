    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Boven de bovenbouw uitstekende draagconstructie voor tuien (kabels).</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverbruggingsdeel"/>
  </Concept>
  <ConceptScheme xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerk">
