    <prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">opslagtank</prefLabel>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Opslagfaciliteit voor vloeistoffen, gassen of energie.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerk"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerk_bezinkbak">
