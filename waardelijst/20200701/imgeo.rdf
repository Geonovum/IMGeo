    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Paal waaraan een of meerdere verkeersborden zijn bevestigd.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus"/>
