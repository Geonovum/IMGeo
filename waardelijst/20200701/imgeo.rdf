    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Een afzonderlijk staande overdekking rustend op kolommen.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverigBouwwerk"/>
