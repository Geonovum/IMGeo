    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Paal met daarop de dienstregeling ten behoeve van het openbaar vervoer.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus"/>
