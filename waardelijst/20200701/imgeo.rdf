    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Direct door het verkeer belaste deel van de bovenbouw van de brug.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverbruggingsdeel"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypeOverbruggingsdeel_landhoofd">
