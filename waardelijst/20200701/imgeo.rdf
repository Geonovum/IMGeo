    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus_telpaal">
    <type xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">telpaal</prefLabel>
    <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Paal waaraan de verkeersteller is bevestigd.</definition>
    <inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus"/>
  </Concept>
  <Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/2.2#TypePaalPlus_portaal">
    <type xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">portaal</prefLabel>
